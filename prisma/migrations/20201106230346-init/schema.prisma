// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model Area {
  id            String      @id @default(uuid())
  region        String
  name          String
  slug          String      @unique
  parentAreaId  String?
  latitude      Decimal
  longitude     Decimal
  order         Int
  isDisabled    Boolean
  isDeleted     Boolean
}